<h1 class="my-2">Store Index</h1>

<table class="table table-striped">
  <thead>
    <tr>
      <th scope="col">Id</th>
      <th scope="col">Name</th>
      <th scope="col">Address</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <% @stores.each do |store| %>
        <th scope="row"><%= store.id %></th>
        <td><%= store.name %></td>
        <td><%= store.address %></td>>
    </tr>
    <% end %>
  </tbody>
</table>
